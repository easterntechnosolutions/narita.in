{{ header }}
<div id="error-not-found" class="container wrapper_container">
  <div class="row">{{ column_left }}
    {% if column_left and column_right %}
    {% set class = 'col-sm-6 column_left' %}
    {% elseif column_left or column_right %}
    {% set class = 'col-sm-9 column_center' %}
    {% else %}
    {% set class = 'col-sm-12 column_full' %}
    {% endif %}
    <div id="content" class="{{ class }}">{{ content_top }}
      <p class="text-center">{{ text_error }}</p>
      {#<div class="buttons clearfix">#}
      {#  <div class="pull-right"><a href="{{ continue }}" class="btn btn-primary">{{ button_continue }}</a></div>#}
      {#</div>#}
      {{ content_middle }}{{ content_bottom }}</div>
    {{ column_right }}</div>
</div>
{{ footer }}
