<section id="{{ ishi_randomnumer }}" class="categoryblock">
    <div class="container">
        <div class="section-header">    
            <h3 class="home-title">{{ heading }}</h3>
        </div>
        <div class="category-content">
            <div class="category-inner">
                <div class="grid-container">
                  {% for banner in banners %}  
                    <div class="categoryblock-container">
                        <div class="category-image">
                            <a href="{{ banner.categorylink }}">
                              <img src="{{ banner.image }}" alt="Category-img" class="img-responsive" />
                              <span class="heading">{{ banner.categorytitle }}</span>
                            </a>
                        </div>  
                    </div>   
                  {% endfor %} 
                </div>
            </div>
        </div>
    </div>
</section>